{
	"name": "WikiRennes",
	"author": [
		"Gabriel Wicke",
		"..."
	],
	"url": "http://www.wiki-rennes.fr",
	"descriptionmsg": "wikirennes-desc",
	"namemsg": "skinname-wikirennes",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.25.0"
	},
	"ValidSkinNames": {
		"wikirennes": "WikiRennes"
	},
	"SkinOOUIThemes": {
		"wikirennes": "Apex"
	},
	"MessagesDirs": {
		"WikiRennes": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.wikirennes.styles": {
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				}
			}
		},
		"skins.wikirennes.js": {
			"scripts": [
				"wikirennes.js"
			],
			"position": "top",
			"dependencies": [
				"mediawiki.toolbar"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "WikiRennes"
	},
	"ResourceModuleSkinStyles": {
		"wikirennes": {
			"+mediawiki.action.view.redirectPage": "resources/skinStyles/mediawiki.action.view.redirectPage.css",
			"+mediawiki.special.preferences.styles": "resources/skinStyles/mediawiki.special.preferences.styles.less",
			"+mediawiki.notification": "resources/skinStyles/mediawiki.notification.less",
			"+oojs-ui-core.styles": "resources/skinStyles/ooui.less"
		}
	},
	"AutoloadClasses": {
		"SkinWikiRennes": "includes/SkinWikiRennes.php",
		"WikiRennesTemplate": "includes/WikiRennesTemplate.php"
	},
	"manifest_version": 1
}
